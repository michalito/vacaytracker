{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"strict": true,
		"moduleResolution": "bundler",
		"allowImportingTsExtensions": true,
		"module": "ESNext",
		"target": "ESNext",
		"lib": ["ESNext", "DOM", "DOM.Iterable"],
		"paths": {
			"$lib/*": ["./src/lib/*"],
			"$components/*": ["./src/lib/components/*"]
		}
	}
}
